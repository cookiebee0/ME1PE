#include <iostream>
using namespace std;
int main()
{
    string s;
    while(cin>>s)
    {
        int ss=s.size();
        for(int i=0;i<ss;i+=3)
        {
            int t=(s[i]-'0')*100+(s[i+1]-'0')*10+(s[i+2]-'0');
            cout<<char(t);
        }
        cout<<'\n';
    }
    return 0;
}
